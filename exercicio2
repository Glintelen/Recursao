#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int i=1, n;
int u(int n);
int u(int n){
    switch (n){
    case 0:
        return 1;
        break;
    case 1:
    return 2;
    break;
    default:
        2 * u(n-1) - 3 * u(n-2);
        break;
    }
}
int main (){
    printf("Digite um número inteiro: ");
    scanf("%i",&n);
   
printf("U(n) é %i",u(n));
return 1;
}
